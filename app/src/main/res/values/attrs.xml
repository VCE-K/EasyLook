<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--音乐条目属性-->
    <declare-styleable name="LocalItemView">
        <attr name="tv_name" format="reference|string" />
        <attr name="iv_icon" format="reference|integer" />
        <attr name="tv_desc" format="reference|string" />
        <attr name="iv_icon_color" format="reference|color" />
    </declare-styleable>


    <declare-styleable name="TypefaceTextView">
        <attr name="typeface">
            <enum name="fzlLTypeface" value="1" />
            <enum name="fzdb1Typeface" value="2" />
            <enum name="futuraTypeface" value="3" />
            <enum name="dinTypeface" value="4" />
            <enum name="lobsterTypeface" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NoStatusFooter">
        <attr name="srlPrimaryColor" />
        <attr name="srlAccentColor" />
        <attr name="srlTextSizeTitle" />
        <attr name="srlTextTitleTypeface">
            <enum name="fzlLTypeface" value="1" />
            <enum name="fzdb1Typeface" value="2" />
            <enum name="futuraTypeface" value="3" />
            <enum name="dinTypeface" value="4" />
            <enum name="lobsterTypeface" value="5" />
        </attr>
        <attr name="srlTextNothing" />
    </declare-styleable>

    <!--圆形进度条控件-->
    <declare-styleable name="CircularProgressBar">
        <!--半径-->
        <attr name="radius" format="dimension" />
        <!--进度条宽度-->
        <attr name="strokeWidth" format="dimension" />
        <!--进度条背景颜色-->
        <attr name="progressbarBackgroundColor" format="color|reference" />
        <!--进度条进度颜色-->
        <attr name="progressbarColor" format="color|reference" />
        <!--最大进度-->
        <attr name="maxProgress" format="integer" />
        <!--当前进度-->
        <attr name="progress" format="integer" />
        <!--文字-->
        <attr name="text" format="string" />
        <!--文字颜色-->
        <attr name="textColor" />
        <!--文字大小-->
        <attr name="textSize" />
    </declare-styleable>

    <!--字体颜色 -->
    <attr name="textColor" format="color" />

    <!--字体颜色 -->
    <attr name="textSize" format="dimension" />

    <!--通用图标色-->
    <attr name="iconColor" format="reference" />
</resources>